    "eslint-plugin-format": "^0.1.0",
    "husky": "^8.0.0",
    "less": "^4.2.0",
    "lint-staged": "^15.2.0",
    "typescript": "^5.3.3",
    "unocss": "^0.58.3",
    "unplugin-auto-import": "^0.17.3",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.0.11",
    "vite-plugin-vue-devtools": "^7.0.10",
    "vue-tsc": "^1.8.27"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx,vue}": "eslint --fix"
  }
